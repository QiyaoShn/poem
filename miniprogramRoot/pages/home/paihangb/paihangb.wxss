page {
  background-color: #46AEF0;
}

.tabNav {
  z-index: 4;
  position: fixed;
  top: 40rpx;
  left: 55rpx;
  height: 90rpx;
  line-height: 90rpx;
  display: flex;
  justify-content: space-around;
  padding: 0rpx 100rpx;
  border-bottom: 1px solid #f5f5f5;
  border-top: 1px solid #f5f5f5;
  border-left: 1px solid #f5f5f5;
  border-right: 1px solid #f5f5f5;
  border-radius: 25px 25px 25px 25px;
  box-sizing: border-box;
  color: white;
  font-size: 33rpx;
}

.tabNav>view {
  text-align: center;
  margin-right: 50rpx;
}

.tabNav>view:last-child {
  margin-right: 0;
}

.tabNav>view text {
  padding: 0rpx 15rpx;
  height: 88rpx;
  display: inline-block;
}

.tabNav .cur text {
  color: #46AEF0;
  background-color: white;
}

.bang-dan {
  display: flex;
  flex-direction: column;
  margin: 90px 10px 15px 10px;
  background-color: white;
  border-radius: 10px 10px 10px 10px;
  border-width: 2px;
}
.NO3-bang{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}
.NO4-bang{
  display: flex;
  flex-direction: column;
  margin-right: 350rpx;
  align-items: center;
}
.tou-xiang{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.mo-ban{
  display: flex;
  flex-direction: column;
